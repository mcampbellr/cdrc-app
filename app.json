{
  "expo": {
    "name": "cdrc-app",
    "slug": "cdrc-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "cdrc",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.mcampbellr.cdrcapp",
      "usesAppleSignIn": true,
      "appleTeamId": "D8X2KM25L7"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "com.mcampbellr.cdrcapp"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 150,
          "resizeMode": "contain",
          "backgroundColor": "#121210"
        }
      ],
      "expo-secure-store",
      "expo-apple-authentication"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c0935bf0-6f98-47df-be06-1770fb424f18"
      }
    },
    "owner": "mcampbellr"
  }
}
